@{
    ViewData["Title"] = "Home Page";
}
<div class="card" role="main" aria-labelledby="pageTitle">
    <div class="header">
        <h1 id="pageTitle" class="title">Student Master — Mark Sheet</h1>
        <p class="subtitle">Simple, clean and centered layout for quick viewing.</p>
    </div>

    <div class="table-wrap" aria-live="polite">
        <table class="styled" role="table">
            <caption>Marks for semester</caption>
            <thead>
                <tr>
                    <th scope="col">Subject</th>
                    <th scope="col">Meet</th>
