                    <th scope="col">Harshil</th>
                    <th scope="col">Priya</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Python</td>
                    <td>80</td>
                    <td>89</td>
                    <td>86</td>
                    <td>85</td>
                </tr>
                <tr>
                    <td>Java</td>
                    <td>89</td>
                    <td>82</td>
                    <td>90</td>
                    <td>88</td>
                </tr>
                <tr>
                    <td>Web Program</td>
                    <td>90</td>
                    <td>89</td>
                    <td>88</td>
                    <td>87</td>
                </tr>
            </tbody>
            <tfoot>
                <tr>
                    <th>Total</th>
                    <th>259</th>
                    <th>260</th>
                    <th>264</th>
                    <th>260</th>
                </tr>
                <tr>
                    <th>Average</th>
                    <th>86.33</th>
                    <th>86.67</th>
                    <th>88.00</th>
                    <th>86.67</th>
                </tr>
            </tfoot>
        </table>
    </div>
</div>
